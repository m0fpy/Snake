<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Name="BackgroundColor">#4C4C4C</SolidColorBrush>
        <SolidColorBrush x:Name="GridColor">#727272</SolidColorBrush>
        <SolidColorBrush x:Name="GridLines">#2E2E2E</SolidColorBrush>
        <SolidColorBrush x:Name="TextColor">#EBEBEB</SolidColorBrush>
        <SolidColorBrush x:Name="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Name="MainFont">Assets/#Azeret Mono</FontFamily>
    </Application.Resources>
</Application>
